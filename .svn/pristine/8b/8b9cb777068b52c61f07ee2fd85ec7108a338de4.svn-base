.bg-blue {
    background: linear-gradient(45deg, #0288d1 0%, #26c6da 100%);
  }
  
  .bg-red {
    background: -webkit-linear-gradient(45deg, #FF5252 0%, #f48fb1 100%);
  }
  
  .bg-orange {
    background: linear-gradient(45deg, #ff6f00 0%, #ffca28 100%);
  }
  
  .bg-green {
    background: linear-gradient(45deg, #43A047 0%, #1de9b6 100%);
  }
  
  .cursor-pointer {
    cursor: pointer;
  }
  
  .font_9b {
    font-size: 9px;
    font-weight: bold;
  }
  
  .font_10 {
    font-size: 10px;
  }
  
  .font_11b {
    font-size: 11px;
    font-weight: bold;
  }
  
  .font_12b {
    font-size: 12px;
    font-weight: bold;
  }
  
  .font_13b {
    font-size: 13px;
    font-weight: bold;
  }
  
  .font_17b {
    font-size: 17px;
    font-weight: bold;
  }
  
  .myClass .mat-dialog-container { 
    padding: 0;
    overflow: hidden;
  }
  
  .myClass .mat-dialog-content { 
    margin: 0;
  }
  
  .myClass .mat-dialog-actions { 
    margin-bottom: 0px !important;
    padding: 20px 30px;
  }
  
  .premium-breakup-table {
    width: 100%;
    font-size: 11px;
  }
  
  .font_12b label {
      margin-bottom: 0px;
  }
  
  .premium-card-footer-bg .mat-checkbox-frame {
      border-color: #fff;
  }
  
  .font_12b .mat-checkbox-inner-container {
      width: 16px;
      height: 16px;
  }
  
  .mat-expansion-indicator::after {
      color: #0288d1;
  }
  
  .mat-toolbar-row, .mat-toolbar-single-row {
    height: 46px !important;
  }
  
  .compare-premium th {
    width: 50px;
    max-width: 50px;
  }
  
  .car-edit-fab-left {
    position: fixed !important;
    bottom: 10px;
  }
  
  .filter-fab-right {
    position: fixed !important;
    bottom: 10px;
    right: 15px;
  }
  
  .fancy-drawer{
    min-width: 300px;
    /* padding: 15px; */
  }
  
  /* XS Screen*/
  
  @media screen and (max-width: 767px) {
    .compare-img-dlg-img {
      width: 63px;
    }
    .compare-buy-now-xs-btn {
      font-size: 11px;
      padding: 0px !important;
      min-width: 63px !important;
    }
  
    .compare-table-td-xs {
      font-size: 11px;
    }
  }
  