/*First Section Start*/

/* Header Start */

.logo {
  width: 120px;
}

.header {
  padding: 10px 0px;
  width: 100%;
  height: auto;
  position: absolute;
  /* top: 52px; */
}

/* .header .navbar-brand img {
  display: none;
} */

.header .navbar-brand {
  margin-left: 35px;
}

.header .nav-link {
  padding: 5px 20px !important;
  color: #391f77;
  font-weight: 600;
  text-transform: uppercase;
  font-size: 13px;
}

.header_dropdown {
  background-color: #ececec42;
  border: 1px solid rgba(0, 0, 0, 0.03);
  padding: 0px;
  margin-left: 20px;
}

.header_dropdown .dropdown-item {
  padding: 5px 8px;
  color: #391f77;
  font-size: 13px;
}

.sticky {
  position: fixed;
  top: 0;
  width: 100%;
  background: rgba(255, 255, 255, .4);
}

.menuIcon {
  display: inline-block;
  cursor: pointer;
}

.bar1,
.bar2,
.bar3 {
  width: 20px;
  height: 2px;
  background-color: #03c;
  margin: 5px 0;
  transition: 0.4s;
}

.change .bar1 {
  -moz-transform: rotate(-45deg) translate(-2px, 2px);
  -webkit-transform: rotate(-45deg) translate(-2px, 2px);
  transform: rotate(-45deg) translate(-2px, 2px);
}

.change .bar2 {
  opacity: 0;
}

.change .bar3 {
  -moz-transform: rotate(45deg) translate(-8px, -8px);
  -webkit-transform: rotate(45deg) translate(-8px, -8px);
  transform: rotate(45deg) translate(-8px, -8px);
}

/* Header End */

.home_main_content {
  width: 100%;
  min-height: 100vh;
  position: relative;
  background-image: url("../../../../assets/images/landing_bg.jpg");
  background-position: center;
  background-size: cover;
  margin-top: -52px;
  padding-top: 130px;
  margin-bottom: -4px;
}

.home_main_content::before {
  content: "";
  position: absolute;
  opacity: 0.2;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
  z-index: 0;
  background: linear-gradient(0deg, rgb(142, 36, 170) 20%, rgb(255, 110, 64) 50%);
}

/* Balloon style start */

.color-white {
  color: #fff !important;
}

.balloon {
  display: inline-block;
  width: 138px;
  height: 138px;
  /* background:hsl(215,50%,65%); */
  background: linear-gradient(45deg, #0288d1 0%, #26c6da 100%);
  border-radius: 80%;
  position: relative;
  box-shadow: inset -10px -10px 0 rgba(0, 0, 0, 0.07);
  margin: 20px 30px;
  transition: transform 0.5s ease;
  z-index: 10;
  /* animation: balloons 4s ease-in-out infinite; */
  transform-origin: bottom center;
}

/* @keyframes balloons {
      0%,
      100% {
          transform: translateY(0) rotate(-4deg);
      }
      50% {
          transform: translateY(-25px) rotate(4deg);
      }
  } */

.balloon:hover {
  cursor: pointer;
}

.balloon:before {
  content: "â–²";
  font-size: 20px;
  color: #0288d1;
  display: block;
  text-align: center;
  width: 100%;
  position: absolute;
  bottom: -12px;
  z-index: -100;
}

.balloon:nth-child(2) {
  background: hsl(245, 40%, 65%);
  background: linear-gradient(45deg, #FF5252 0%, #f48fb1 100%);
  animation-duration: 3.5s;
}

.balloon:nth-child(2):before {
  color: #FF5252;
}

.balloon:nth-child(3) {
  background: hsl(139, 50%, 60%);
  background: linear-gradient(45deg, #ff6f00 0%, #ffca28 100%);
  animation-duration: 3s;
}

.balloon:nth-child(3):before {
  color: #ff6f00;
}

.balloon:nth-child(4) {
  background: hsl(59, 50%, 58%);
  background: linear-gradient(45deg, #43A047 0%, #1de9b6 100%);
  animation-duration: 4.5s;
}

.balloon:nth-child(4):before {
  color: #43A047;
}

.balloon:nth-child(5) {
  background: hsl(23, 55%, 57%);
  background: linear-gradient(45deg, #EC6F66 0%, #F3A183 100%);
  animation-duration: 5s;
}

.balloon:nth-child(5):before {
  color: #EC6F66;
}

.balloon:nth-child(6) {
  background: hsl(23, 55%, 57%);
  background: linear-gradient(45deg, #D38312 0%, #A83279 100%);
  animation-duration: 5s;
}

.balloon:nth-child(6):before {
  color: #D38312;
}

.balloon:nth-child(7) {
  background: linear-gradient(45deg, #6f3ebd 0%, #6d8cc3 100%);
  animation-duration: 5s;
}

.balloon:nth-child(7):before {
  color: #6f3ebd;
}

.balloon:nth-child(8) {
  background: linear-gradient(45deg, #e25b74 0%, #e67de3 100%);
  animation-duration: 5s;
}

.balloon:nth-child(8):before {
  color: #e25b74;
}

.balloon:nth-child(9) {
  background: linear-gradient(45deg, #f10909 0%, #f95fa6 100%);
  animation-duration: 5s;
}

.balloon:nth-child(9):before {
  color: #f10909;
}

/* Balloon style End */

