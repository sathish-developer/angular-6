/* Footer Section Start */

.footer {
  width: 100%;
  height: auto;
  background: #0c0114;
  color: #fff;
  display: inline-block;
}

.footer_first {
  width: 100%;
  height: auto;
  border-bottom: 1px solid #eeeeee6e;
}

.footer_first h3 {
  font-size: 20px;
  color: #e4e4e4;
}

.footer ul {
  list-style: none;
  list-style: none;
  padding: 0px;
}

.footer ul li {
  color: #ccc;
  font-size: 13px;
}

.footer ul li a {
  color: #ccc;
  text-decoration: none;
  font-size: 13px;
}

.footer ul li i {
  margin-right: 2px;
}

.footer .social_icons li {
  display: inline-block;
  font-size: 0px;
  cursor: pointer;
  margin: 10px 10px;
  width: 45px;
  height: 45px;
  border-radius: 50%;
  text-align: center;
  position: relative;
  z-index: 1;
  color: #fff;
  border: 2px solid #fff;
}

.footer .social_icons li a {
  color: #fff;
}

.footer .social_icons li:hover {
  border: 2px solid rgba(255, 255, 255, 0);
}

.footer .social_icons li:after {
  pointer-events: none;
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  content: '';
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
  top: -2px;
  left: -2px;
  padding: 0;
  z-index: 10;
  border: 2px dashed #fff;
}

.footer .social_icons li:hover:after {
  -webkit-animation: rotation 9s linear infinite;
  -moz-animation: rotation 9s linear infinite;
  animation: rotation 9s linear infinite;
}

@-webkit-keyframes rotation {
  from {
    -webkit-transform: rotate(0deg);
  }
  to {
    -webkit-transform: rotate(359deg);
  }
}

.footer .social_icons li i {
  font-size: 20px;
  line-height: 36px;
  margin: 0px;
}

.footer .footer_para {
  width: 80%;
  color: #c7c7c7;
  font-size: 12px;
  line-height: 16px;
  margin-bottom: 0px;
  margin: auto;
  letter-spacing: 0.5px;
}

.footer .copy_right {
  background: #353535;
  font-size: 13px;
  letter-spacing: 0.5px;
}

.fa-phone-rotate {
  transform: rotate(90deg);
}

#goTop {
  display: none;
  position: fixed;
  bottom: 20px;
  right: 30px;
  z-index: 99;
  font-size: 24px;
  border: none;
  outline: none;
  background-color: rgba(208, 192, 192, 0.47);
  color: white;
  cursor: pointer;
  padding: 9px;
  width: 55px;
  border-radius: 50%;
}

#goTop:hover {
  background-color: rgba(212, 211, 205, 0.55);
}

